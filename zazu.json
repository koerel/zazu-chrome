{
  "name": "zazu-template",
  "version": "1.0.0",
  "description": "Zazu Template for starting your own plugin.",
  "blocks": {
    "input": [
      {
        "id": "open-chrome",
        "type": "PrefixScript",
        "prefix": "chr",
        "space": true,
        "args": "Required",
        "icon": "src/assets/chrome.png",
        "script": "src/open.js",
        "connections": [2]
      }
    ],
    "output": [
      {
        "id": 2,
        "type": "OpenInBrowser",
        "url": "{value}"
      }
    ]
  }
}