{
  "name": "zazu-template",
  "version": "1.0.0",
  "description": "Zazu Template for starting your own plugin.",
  "blocks": {
    "input": [
      {
        "id": "open-chrome",
        "type": "PrefixScript",
        "prefix": ",c",
        "icon": "src/assets/chrome.png",
        "script": "src/open.js"
        "connections": [
          "Open"
        ]
      },
    ],
    "output": [
      {
        "id": "Open",
        "type": "OpenInBrowser",
        "url": "{value}"
      }
    ]
  }
}